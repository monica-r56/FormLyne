  "scripts": {
    "build": "cross-env yarn run build:clean && yarn run lokalise && webpack --config webpack.production.ts",
    "start": "webpack serve --config webpack.development.ts --open",
    "storybook": "cross-env STORYBOOK_APP_ENV=storybook storybook dev -p 6006",
    "build-storybook": "cross-env STORYBOOK_APP_ENV=storybook storybook build",
    "test": "yarn lint && cross-env NODE_ENV=test TZ=UTC NODE_OPTIONS=--trace-deprecation jest --coverage ",
    "test:watch": "cross-env NODE_ENV=test TZ=UTC jest --watchAll",
    "lint": "eslint app",
    "lint:fix": "yarn lint --fix",
    "pretty": "prettier --write .",
    "prepare": "cross-env husky",
    "build:clean": "rimraf ./build",
    "test-storybook:ci": "yarn test-storybook --maxWorkers=2",
    "extract": "formatjs extract 'app/**/*.ts*' --out-file temp.json --flatten --format lokalise --ignore='**/*.d.ts'",
    "compile": "formatjs compile 'temp.json' --out-file app/locale/en.json --format lokalise && rm temp.json",
    "extract-intl": "yarn extract && yarn compile",
    "lokalise": "dotenv -e .env ./import_translations.sh",
    "upload-lokalise": "dotenv -e .env ./upload_translations.sh",
    "chromatic": "cross-env STORYBOOK_APP_ENV=storybook chromatic --exit-zero-on-changes",
    "lint-staged": "lint-staged",
    "update-translations": "cross-env yarn run lokalise"
  },
